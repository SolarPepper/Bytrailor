# Bybit API keys (required)
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here

# Trading settings (optional, recommended defaults)
TAKE_PROFIT_PERCENT=5              # Take-profit as percent of current price (recommended: 5)
STOP_LOSS_PERCENT=-2.5             # Stop-loss as percent of current price (recommended: -2.5)
TRAILING_START_PERCENT=1.6         # Profit percent to start trailing (recommended: 1.6)
TRAILING_DISTANCE_PERCENT=0.8      # Trailing distance from current price in % (recommended: 0.8)

# Network (set to true for testnet, false for mainnet)
BYBIT_TESTNET=false

